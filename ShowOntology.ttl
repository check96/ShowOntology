@prefix : <http://www.semanticweb.org/gaetano/ontologies/ShowOntology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix so: <http://www.semanticweb.org/gaetano/ontologies/ShowOntology#> .
@prefix bag: <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix list: <http://www.ontologydesignpatterns.org/cp/owl/list.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spec: <http://xmlns.com/foaf/spec/#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <https://vocab.org/vann/#> .
@prefix foaf1: <http://xmlns.com/foaf/> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix bbc-po: <http://purl.org/ontology/po/> .
@prefix sequence: <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#> .
@prefix collectionentity: <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/gaetano/ontologies/ShowOntology> .

<http://www.semanticweb.org/gaetano/ontologies/ShowOntology> rdf:type owl:Ontology ;
                                                              owl:imports <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                                              dc:creator "Gaetano Donadio" ;
                                                              dc:date "07-09-2020" ;
                                                              dc:description "Ontologia per la descrizione e rappresentazione di entità relative al mondo cinematografico e televisivo. Sono rappresentati programmi come Film e Serie Tv e la relativa valutazione: recensioni e premi attribuiti."@it ;
                                                              dc:title "Show Ontology" ;
                                                              rdfs:label "Show Ontology" ;
                                                              vann:preferredNamespacePrefix "so" ;
                                                              vann:preferredNamespaceUri "http://www.semanticweb.org/gaetano/ontologies/ShowOntology#" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  https://vocab.org/vann/#preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  https://vocab.org/vann/#preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#xsd:date
so:xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/ontology/po/director
bbc-po:director rdf:type owl:ObjectProperty ;
                rdfs:comment "Relates a programme to its supervisor. Generally refers to the person responsible for all audience-visible components of a program, film, or show, whereas the producer is responsible for the financial and other behind-the-scenes aspects. A director's duties might also include casting, script editing, shot selection, shot composition, and editing"@en ;
                rdfs:label "director"@en .


###  http://purl.org/ontology/po/genre
bbc-po:genre rdf:type owl:ObjectProperty ;
             rdfs:domain bbc-po:Programme ;
             rdfs:range bbc-po:Genre ;
             rdfs:comment "Relates a programme to a particular genre."@en ;
             rdfs:label "genre"@en .


###  http://purl.org/ontology/po/producer
bbc-po:producer rdf:type owl:ObjectProperty ;
                rdfs:comment "Relates a programme to its producer - the manager of an event, show, or other work, usually the individual in charge of finance, personnel, and other non-artistic aspects in the development of commercials, plays, movies, and other works"@en ;
                rdfs:label "producer"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#act_in
so:act_in rdf:type owl:ObjectProperty ;
          rdfs:domain <http://dbpedia.org/ontology/Actor> ;
          rdfs:range bbc-po:Programme ;
          owl:propertyChainAxiom ( so:plays
                                   so:characterOf
                                 ) ;
          rdfs:comment "Relazione tra un attore e il programma (FIlm o Serie Tv) in cui recita."@it ;
          rdfs:label "act_in"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#appears_in
so:appears_in rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain <http://dbpedia.org/ontology/FictionalCharacter> ;
              rdfs:range bbc-po:Episode ;
              rdfs:comment "Relazione tra un personaggio e l'episodio in cui appare" ;
              rdfs:label "appears_in"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#assigned_in
so:assigned_in rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain so:Award ;
               rdfs:range so:FestivalEdition ;
               rdfs:comment "Indica la relazione tra un premio e l'edizione del festival a cui è stato assegnato"@it ;
               rdfs:label "assigned_in"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#candidatedTo
so:candidatedTo rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf so:hasCandidate ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( <http://dbpedia.org/ontology/Actor>
                                            bbc-po:Programme
                                            so:Director
                                          )
                            ] ;
                rdfs:range so:Award ;
                rdfs:comment """Relazione tra un Programma, un Attore o un Regista e il premio per cui è candidato alla vittoria.
Relazione inversa di hasCandidated"""@en ;
                rdfs:label "candidatedTo"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#characterOf
so:characterOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf so:hasCharacter ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain <http://dbpedia.org/ontology/FictionalCharacter> ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( so:Film
                                          so:TvSeries
                                        )
                          ] ;
               rdfs:comment "Relazione tra un personaggio e il film o la serie tv in cui compare"@it ;
               rdfs:label "characterOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#directedBy
so:directedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf bbc-po:director ;
              owl:inverseOf so:directs ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( bbc-po:Episode
                                          so:Film
                                        )
                          ] ;
              rdfs:range so:Director ;
              rdfs:comment """Relazione tra un episodio o un film e il regista che l'ha diretto.
Inversa di directs"""@it ;
              rdfs:label "directedBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#directs
so:directs rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain so:Director ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( bbc-po:Episode
                                      so:Film
                                    )
                      ] ;
           rdfs:comment """Relazione tra il regista e l'episodio o film che ha diretto.
Inversa di directedBy"""@it ;
           rdfs:label "directs"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#editionOf
so:editionOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf so:hasEdition ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain so:FestivalEdition ;
             rdfs:range so:AwardsFestival ;
             rdfs:comment "Indica la relazione tra un'edizione di una competizione e la competizione stessa"@it ;
             rdfs:label "editionOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#episodeOf
so:episodeOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf bag:itemOf ;
             owl:inverseOf so:hasEpisode ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain bbc-po:Episode ;
             rdfs:range bbc-po:Season ;
             rdfs:comment "Relazione tra un episodio e la stagione di cui fa parte"@it ;
             rdfs:label "episodeOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#episodeOfSeries
so:episodeOfSeries rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain bbc-po:Episode ;
                   rdfs:range so:TvSeries ;
                   owl:propertyChainAxiom ( so:episodeOf
                                            so:seasonOf
                                          ) ;
                   rdfs:comment "Relazione tra un episodio e la serie tv di cui fa parte" ;
                   rdfs:label "episodeOfSeries"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#firstEpisodeOf
so:firstEpisodeOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf list:firstItemOf ,
                                     so:episodeOf ;
                  owl:inverseOf so:hasFirstEpisode ;
                  rdf:type owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty ;
                  rdfs:domain bbc-po:Episode ;
                  rdfs:range bbc-po:Season ;
                  rdfs:comment "Relazione inversa di hasFirstEpisode"@it ;
                  rdfs:label "firstEpisodeOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#firstSeasonOf
so:firstSeasonOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf list:firstItemOf ,
                                    so:seasonOf ;
                 owl:inverseOf so:hasFirstSeason ;
                 rdf:type owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain bbc-po:Season ;
                 rdfs:range so:TvSeries ;
                 rdfs:comment "Relazione inversa di hasFirstSeason"@it ;
                 rdfs:label "firstSeasonOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#followsEpisode
so:followsEpisode rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sequence:follows ;
                  owl:inverseOf so:precedesEpisode ;
                  rdf:type owl:TransitiveProperty ;
                  rdfs:domain bbc-po:Episode ;
                  rdfs:range bbc-po:Episode ;
                  rdfs:comment "Relazione tra un episodio e un episodio precedente ad esso."@it ;
                  rdfs:label "followsEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#followsSeason
so:followsSeason rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sequence:follows ;
                 owl:inverseOf so:precedesSeason ;
                 rdf:type owl:TransitiveProperty ;
                 rdfs:domain bbc-po:Season ;
                 rdfs:range bbc-po:Season ;
                 rdfs:comment "Relazione tra una stagione e un'altra precedente ad essa.."@it ;
                 rdfs:label "followsSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasCandidate
so:hasCandidate rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain so:Award ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( <http://dbpedia.org/ontology/Actor>
                                           bbc-po:Programme
                                           so:Director
                                         )
                           ] ;
                rdfs:comment """Relazione tra un premio e un candidato a vincerlo.
Inversa di candidatedTo"""@it ;
                rdfs:label "hasCandidate"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasCharacter
so:hasCharacter rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:InverseFunctionalProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( so:Film
                                            so:TvSeries
                                          )
                            ] ;
                rdfs:range <http://dbpedia.org/ontology/FictionalCharacter> ;
                rdfs:comment """Relazione tra un film o una serie Tv e i personaggi che appaiono in esso/a.
Inversa di characterOf"""@it ;
                rdfs:label "hasCharacter"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasEdition
so:hasEdition rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:InverseFunctionalProperty ;
              rdfs:domain so:AwardsFestival ;
              rdfs:range so:FestivalEdition ;
              rdfs:comment """Relazione tra una competizione a premi e un'edizione di essa.
Inversa di editionOf"""@it ;
              rdfs:label "hasEdition"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasEpisode
so:hasEpisode rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf bag:hasItem ;
              rdf:type owl:InverseFunctionalProperty ;
              rdfs:domain bbc-po:Season ;
              rdfs:range bbc-po:Episode ;
              rdfs:comment "Relazione tra una stagione e un episodio"@it ;
              rdfs:label "hasEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasFirstEpisode
so:hasFirstEpisode rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf list:hasFirstItem ,
                                      so:hasEpisode ;
                   rdf:type owl:FunctionalProperty ,
                            owl:InverseFunctionalProperty ;
                   rdfs:domain bbc-po:Season ;
                   rdfs:range bbc-po:Episode ;
                   rdfs:comment "Relazione tra una stagione e il suo primo episodio"@it ;
                   rdfs:label "hasFirstEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasFirstSeason
so:hasFirstSeason rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf list:hasFirstItem ,
                                     so:hasSeason ;
                  rdf:type owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty ;
                  rdfs:domain so:TvSeries ;
                  rdfs:range bbc-po:Season ;
                  rdfs:comment "Relazione tra una serie e la sua prima stagione"@it ;
                  rdfs:label "hasFirstSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasLastEpisode
so:hasLastEpisode rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf list:hasLastItem ,
                                     so:hasEpisode ;
                  owl:inverseOf so:lastEpisodeOf ;
                  rdf:type owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty ;
                  rdfs:domain bbc-po:Season ;
                  rdfs:range bbc-po:Episode ;
                  rdfs:comment "Relazione tra una stagione e il suo ultimo episodio"@it ;
                  rdfs:label "hasLastEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasLastSeason
so:hasLastSeason rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf list:hasLastItem ,
                                    so:hasSeason ;
                 owl:inverseOf so:lastSeasonOf ;
                 rdf:type owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain so:TvSeries ;
                 rdfs:range bbc-po:Season ;
                 rdfs:comment "Relazione tra una serie e la sua ultima stagione"@it ;
                 rdfs:label "hasLastSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasNextEpisode
so:hasNextEpisode rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sequence:directlyPrecedes ,
                                     so:precedesEpisode ;
                  owl:inverseOf so:hasPreviousEpisode ;
                  rdf:type owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain bbc-po:Episode ;
                  rdfs:range bbc-po:Episode ;
                  rdfs:comment "Relazione tra un episodio e quello immediatemente successivo"@it ;
                  rdfs:label "hasNextEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasNextSeason
so:hasNextSeason rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sequence:directlyPrecedes ,
                                    so:precedesSeason ;
                 owl:inverseOf so:hasPreviousSeason ;
                 rdf:type owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain bbc-po:Season ;
                 rdfs:range bbc-po:Season ;
                 rdfs:comment "Relazione tra una stagione e quella immediatemente successiva"@it ;
                 rdfs:label "hasNextSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasPreviousEpisode
so:hasPreviousEpisode rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sequence:directlyFollows ,
                                         so:followsEpisode ;
                      rdf:type owl:FunctionalProperty ,
                               owl:InverseFunctionalProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain bbc-po:Episode ;
                      rdfs:range bbc-po:Episode ;
                      rdfs:comment "Relazione tra un episodio e quello immediatemente precedente"@it ;
                      rdfs:label "hasPreviousEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasPreviousSeason
so:hasPreviousSeason rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf sequence:directlyFollows ,
                                        so:followsSeason ;
                     rdf:type owl:FunctionalProperty ,
                              owl:InverseFunctionalProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain bbc-po:Season ;
                     rdfs:range bbc-po:Season ;
                     rdfs:comment "Relazione tra una stagione e quella immediatemente precedente"@it ;
                     rdfs:label "hasPreviousSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasReview
so:hasReview rdf:type owl:ObjectProperty ;
             owl:inverseOf so:reviewOf ;
             rdf:type owl:InverseFunctionalProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( bbc-po:Episode
                                         so:Film
                                       )
                         ] ;
             rdfs:range so:Review ;
             rdfs:comment "Relazione tra l'episodio o il film e la relativa recensione"@it ;
             rdfs:label "hasReview"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#hasSeason
so:hasSeason rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf bag:hasItem ;
             owl:inverseOf so:seasonOf ;
             rdf:type owl:InverseFunctionalProperty ;
             rdfs:domain so:TvSeries ;
             rdfs:range bbc-po:Season ;
             rdfs:comment "Relazione tra una serie tv e una stagione"@it ;
             rdfs:label "hasSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#lastEpisodeOf
so:lastEpisodeOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf list:lastItemOf ,
                                    so:episodeOf ;
                 rdf:type owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain bbc-po:Episode ;
                 rdfs:range bbc-po:Season ;
                 rdfs:comment "Relazione inversa di hasLastEpisode"@it ;
                 rdfs:label "lastEpisodeOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#lastSeasonOf
so:lastSeasonOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf list:lastItemOf ,
                                   so:seasonOf ;
                rdf:type owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:domain bbc-po:Season ;
                rdfs:range so:TvSeries ;
                rdfs:comment "Relazione inversa di hasLastSeason"@it ;
                rdfs:label "lastSeasonOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#playedBy
so:playedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf so:plays ;
            rdfs:domain <http://dbpedia.org/ontology/FictionalCharacter> ;
            rdfs:range <http://dbpedia.org/ontology/Actor> ;
            rdfs:comment """Relazione tra un personaggio e l'attore che lo interpreta.
Inversa di plays."""@it ;
            rdfs:label "playedBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#plays
so:plays rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain <http://dbpedia.org/ontology/Actor> ;
         rdfs:range <http://dbpedia.org/ontology/FictionalCharacter> ;
         rdfs:comment """Relazione tra un attore e il personaggio che interpreta.
Inversa di playedBy"""@it ;
         rdfs:label "plays"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#precedesEpisode
so:precedesEpisode rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sequence:precedes ;
                   rdf:type owl:TransitiveProperty ;
                   rdfs:domain bbc-po:Episode ;
                   rdfs:range bbc-po:Episode ;
                   rdfs:comment "Relazione tra un episodio e un episodio successivo ad esso."@it ;
                   rdfs:label "precedesEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#precedesSeason
so:precedesSeason rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sequence:precedes ;
                  rdf:type owl:TransitiveProperty ;
                  rdfs:domain bbc-po:Season ;
                  rdfs:range bbc-po:Season ;
                  rdfs:comment "Relazione tra una stagione e un'altra successiva ad essa."@it ;
                  rdfs:label "precedesSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#produces
so:produces rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf so:productedBy ;
            rdfs:domain <http://dbpedia.org/ontology/Producer> ;
            rdfs:range bbc-po:Programme ;
            rdfs:comment """Relazione tra un produttore e uno show.
Inversa di productedBy"""@it ;
            rdfs:label "produces"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#productedBy
so:productedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bbc-po:producer ;
               rdfs:domain bbc-po:Programme ;
               rdfs:range <http://dbpedia.org/ontology/Producer> ;
               rdfs:comment "Relazione tra uno show e il suo produttore"@it ;
               rdfs:label "productedBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#receivedAward
so:receivedAward rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf so:candidatedTo ;
                 owl:inverseOf so:wonBy ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( <http://dbpedia.org/ontology/Actor>
                                             bbc-po:Programme
                                             so:Director
                                           )
                             ] ;
                 rdfs:range so:Award ;
                 rdfs:comment "Relazione tra il programma, l'attore o il regista e il premio che ha ricevuto"@it ;
                 rdfs:label "receivedAward"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#reviewOf
so:reviewOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain so:Review ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( bbc-po:Episode
                                       so:Film
                                     )
                       ] ;
            rdfs:comment "Relazione tra una recensione e l'episodio o film a cui si riferisce"@it ;
            rdfs:label "reviewOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#reviewWrittenBy
so:reviewWrittenBy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   owl:inverseOf so:wroteReview ;
                   rdfs:domain so:Review ;
                   rdfs:range so:Reviewer ;
