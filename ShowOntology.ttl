@prefix : <http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix so: <http://www.semanticweb.org/gaetano/ontologies/ShowOntology#> .
@prefix bag: <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix list: <http://www.ontologydesignpatterns.org/cp/owl/list.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spec: <http://xmlns.com/foaf/spec/#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <https://vocab.org/vann/#> .
@prefix foaf1: <http://xmlns.com/foaf/> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix bbc-po: <http://purl.org/ontology/po/> .
@prefix sequence: <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#> .
@prefix collectionentity: <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology> .

<http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology> rdf:type owl:Ontology ;
                                                                     owl:imports <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                                                     dc:creator "Gaetano Donadio" ;
                                                                     dc:date "07-09-2020" ;
                                                                     dc:description "Ontologia per la descrizione e rappresentazione di entità relative al mondo cinematografico e televisivo. Sono rappresentati programmi come Film e Serie Tv e la relativa valutazione: recensioni e premi attribuiti."@it ;
                                                                     dc:title "Show Ontology" ;
                                                                     rdfs:label "Show Ontology" ;
                                                                     vann:preferredNamespacePrefix "so" ;
                                                                     vann:preferredNamespaceUri "http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  https://vocab.org/vann/#preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  https://vocab.org/vann/#preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#xsd:date
:xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/ontology/po/director
bbc-po:director rdf:type owl:ObjectProperty ;
                rdfs:domain bbc-po:Programme ;
                rdfs:range bbc-po:Person ;
                rdfs:comment "Relates a programme to its supervisor. Generally refers to the person responsible for all audience-visible components of a program, film, or show, whereas the producer is responsible for the financial and other behind-the-scenes aspects. A director's duties might also include casting, script editing, shot selection, shot composition, and editing"@en ;
                rdfs:label "director"@en .


###  http://purl.org/ontology/po/genre
bbc-po:genre rdf:type owl:ObjectProperty ;
             rdfs:domain bbc-po:Programme ;
             rdfs:range bbc-po:Genre ;
             rdfs:comment "Relates a programme to a particular genre."@en ;
             rdfs:label "genre"@en .


###  http://purl.org/ontology/po/producer
bbc-po:producer rdf:type owl:ObjectProperty ;
                rdfs:domain bbc-po:Programme ;
                rdfs:range bbc-po:Person ;
                rdfs:comment "Relates a programme to its producer - the manager of an event, show, or other work, usually the individual in charge of finance, personnel, and other non-artistic aspects in the development of commercials, plays, movies, and other works"@en ;
                rdfs:label "producer"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#act_in
:act_in rdf:type owl:ObjectProperty ;
        rdfs:domain <http://dbpedia.org/ontology/Actor> ;
        rdfs:range bbc-po:Programme ;
        owl:propertyChainAxiom ( :plays
                                 :characterOf
                               ) ;
        rdfs:comment "Relazione tra un attore e il programma (FIlm o Serie Tv) in cui recita."@it ;
        rdfs:label "act_in"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#appears_in
:appears_in rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain <http://dbpedia.org/ontology/FictionalCharacter> ;
            rdfs:range bbc-po:Episode ;
            rdfs:comment "Relazione tra un personaggio e l'episodio in cui appare" ;
            rdfs:label "appears_in"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#assigned_in
:assigned_in rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Award ;
             rdfs:range :FestivalEdition ;
             rdfs:comment "Indica la relazione tra un premio e l'edizione del festival a cui è stato assegnato"@it ;
             rdfs:label "assigned_in"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#candidatedTo
:candidatedTo rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :hasCandidate ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( bbc-po:Person
                                          bbc-po:Programme
                                        )
                          ] ;
              rdfs:range :Award ;
              rdfs:comment """Relazione tra un Programma, un Attore o un Regista e il premio per cui è candidato alla vittoria.
Relazione inversa di hasCandidated"""@en ;
              rdfs:label "candidatedTo"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#characterOf
:characterOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :hasCharacter ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain <http://dbpedia.org/ontology/FictionalCharacter> ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Film
                                        :TvSeries
                                      )
                        ] ;
             rdfs:comment "Relazione tra un personaggio e il film o la serie tv in cui compare"@it ;
             rdfs:label "characterOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#directedBy
:directedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf bbc-po:director ;
            owl:inverseOf :directs ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( bbc-po:Episode
                                        :Film
                                      )
                        ] ;
            rdfs:range :Director ;
            rdfs:comment """Relazione tra un episodio o un film e il regista che l'ha diretto.
Inversa di directs"""@it ;
            rdfs:label "directedBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#directs
:directs rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :Director ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( bbc-po:Episode
                                    :Film
                                  )
                    ] ;
         rdfs:comment """Relazione tra il regista e l'episodio o film che ha diretto.
Inversa di directedBy"""@it ;
         rdfs:label "directs"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#editionOf
:editionOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :hasEdition ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :FestivalEdition ;
           rdfs:range :AwardsFestival ;
           rdfs:comment "Indica la relazione tra un'edizione di una competizione e la competizione stessa"@it ;
           rdfs:label "editionOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeOf
:episodeOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf bag:itemOf ;
           owl:inverseOf :hasEpisode ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain bbc-po:Episode ;
           rdfs:range bbc-po:Season ;
           rdfs:comment "Relazione tra un episodio e la stagione di cui fa parte"@it ;
           rdfs:label "episodeOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeOfSeries
:episodeOfSeries rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain bbc-po:Episode ;
                 rdfs:range :TvSeries ;
                 owl:propertyChainAxiom ( :episodeOf
                                          :seasonOf
                                        ) ;
                 rdfs:comment "Relazione tra un episodio e la serie tv di cui fa parte" ;
                 rdfs:label "episodeOfSeries"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#firstEpisodeOf
:firstEpisodeOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf list:firstItemOf ,
                                   :episodeOf ;
                owl:inverseOf :hasFirstEpisode ;
                rdf:type owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:domain bbc-po:Episode ;
                rdfs:range bbc-po:Season ;
                rdfs:comment "Relazione inversa di hasFirstEpisode"@it ;
                rdfs:label "firstEpisodeOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#firstSeasonOf
:firstSeasonOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf list:firstItemOf ,
                                  :seasonOf ;
               owl:inverseOf :hasFirstSeason ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ;
               rdfs:domain bbc-po:Season ;
               rdfs:range :TvSeries ;
               rdfs:comment "Relazione inversa di hasFirstSeason"@it ;
               rdfs:label "firstSeasonOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#followsEpisode
:followsEpisode rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf sequence:follows ;
                owl:inverseOf :precedesEpisode ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain bbc-po:Episode ;
                rdfs:range bbc-po:Episode ;
                rdfs:comment "Relazione tra un episodio e un episodio precedente ad esso."@it ;
                rdfs:label "followsEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#followsSeason
:followsSeason rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf sequence:follows ;
               owl:inverseOf :precedesSeason ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain bbc-po:Season ;
               rdfs:range bbc-po:Season ;
               rdfs:comment "Relazione tra una stagione e un'altra precedente ad essa.."@it ;
               rdfs:label "followsSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasCandidate
:hasCandidate rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Award ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( bbc-po:Person
                                         bbc-po:Programme
                                       )
                         ] ;
              rdfs:comment """Relazione tra un premio e un candidato a vincerlo.
Inversa di candidatedTo"""@it ;
              rdfs:label "hasCandidate"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasCharacter
:hasCharacter rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:InverseFunctionalProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Film
                                          :TvSeries
                                        )
                          ] ;
              rdfs:range <http://dbpedia.org/ontology/FictionalCharacter> ;
              rdfs:comment """Relazione tra un film o una serie Tv e i personaggi che appaiono in esso/a.
Inversa di characterOf"""@it ;
              rdfs:label "hasCharacter"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasEdition
:hasEdition rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:InverseFunctionalProperty ;
            rdfs:domain :AwardsFestival ;
            rdfs:range :FestivalEdition ;
            rdfs:comment """Relazione tra una competizione a premi e un'edizione di essa.
Inversa di editionOf"""@it ;
            rdfs:label "hasEdition"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasEpisode
:hasEpisode rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf bag:hasItem ;
            rdf:type owl:InverseFunctionalProperty ;
            rdfs:domain bbc-po:Season ;
            rdfs:range bbc-po:Episode ;
            rdfs:comment "Relazione tra una stagione e un episodio"@it ;
            rdfs:label "hasEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasFirstEpisode
:hasFirstEpisode rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf list:hasFirstItem ,
                                    :hasEpisode ;
                 rdf:type owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain bbc-po:Season ;
                 rdfs:range bbc-po:Episode ;
                 rdfs:comment "Relazione tra una stagione e il suo primo episodio"@it ;
                 rdfs:label "hasFirstEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasFirstSeason
:hasFirstSeason rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf list:hasFirstItem ,
                                   :hasSeason ;
                rdf:type owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:domain :TvSeries ;
                rdfs:range bbc-po:Season ;
                rdfs:comment "Relazione tra una serie e la sua prima stagione"@it ;
                rdfs:label "hasFirstSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasLastEpisode
:hasLastEpisode rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf list:hasLastItem ,
                                   :hasEpisode ;
                owl:inverseOf :lastEpisodeOf ;
                rdf:type owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:domain bbc-po:Season ;
                rdfs:range bbc-po:Episode ;
                rdfs:comment "Relazione tra una stagione e il suo ultimo episodio"@it ;
                rdfs:label "hasLastEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasLastSeason
:hasLastSeason rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf list:hasLastItem ,
                                  :hasSeason ;
               owl:inverseOf :lastSeasonOf ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ;
               rdfs:domain :TvSeries ;
               rdfs:range bbc-po:Season ;
               rdfs:comment "Relazione tra una serie e la sua ultima stagione"@it ;
               rdfs:label "hasLastSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasNextEpisode
:hasNextEpisode rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf sequence:directlyPrecedes ,
                                   :precedesEpisode ;
                owl:inverseOf :hasPreviousEpisode ;
                rdf:type owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain bbc-po:Episode ;
                rdfs:range bbc-po:Episode ;
                rdfs:comment "Relazione tra un episodio e quello immediatemente successivo"@it ;
                rdfs:label "hasNextEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasNextSeason
:hasNextSeason rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf sequence:directlyPrecedes ,
                                  :precedesSeason ;
               owl:inverseOf :hasPreviousSeason ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ,
                        owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain bbc-po:Season ;
               rdfs:range bbc-po:Season ;
               rdfs:comment "Relazione tra una stagione e quella immediatemente successiva"@it ;
               rdfs:label "hasNextSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasPreviousEpisode
:hasPreviousEpisode rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf sequence:directlyFollows ,
                                       :followsEpisode ;
                    rdf:type owl:FunctionalProperty ,
                             owl:InverseFunctionalProperty ,
                             owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain bbc-po:Episode ;
                    rdfs:range bbc-po:Episode ;
                    rdfs:comment "Relazione tra un episodio e quello immediatemente precedente"@it ;
                    rdfs:label "hasPreviousEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasPreviousSeason
:hasPreviousSeason rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sequence:directlyFollows ,
                                      :followsSeason ;
                   rdf:type owl:FunctionalProperty ,
                            owl:InverseFunctionalProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain bbc-po:Season ;
                   rdfs:range bbc-po:Season ;
                   rdfs:comment "Relazione tra una stagione e quella immediatemente precedente"@it ;
                   rdfs:label "hasPreviousSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasReview
:hasReview rdf:type owl:ObjectProperty ;
           owl:inverseOf :reviewOf ;
           rdf:type owl:InverseFunctionalProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( bbc-po:Episode
                                       :Film
                                     )
                       ] ;
           rdfs:range :Review ;
           rdfs:comment "Relazione tra l'episodio o il film e la relativa recensione"@it ;
           rdfs:label "hasReview"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasSeason
:hasSeason rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf bag:hasItem ;
           owl:inverseOf :seasonOf ;
           rdf:type owl:InverseFunctionalProperty ;
           rdfs:domain :TvSeries ;
           rdfs:range bbc-po:Season ;
           rdfs:comment "Relazione tra una serie tv e una stagione"@it ;
           rdfs:label "hasSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#lastEpisodeOf
:lastEpisodeOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf list:lastItemOf ,
                                  :episodeOf ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ;
               rdfs:domain bbc-po:Episode ;
               rdfs:range bbc-po:Season ;
               rdfs:comment "Relazione inversa di hasLastEpisode"@it ;
               rdfs:label "lastEpisodeOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#lastSeasonOf
:lastSeasonOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf list:lastItemOf ,
                                 :seasonOf ;
              rdf:type owl:FunctionalProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain bbc-po:Season ;
              rdfs:range :TvSeries ;
              rdfs:comment "Relazione inversa di hasLastSeason"@it ;
              rdfs:label "lastSeasonOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#playedBy
:playedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :plays ;
          rdfs:domain <http://dbpedia.org/ontology/FictionalCharacter> ;
          rdfs:range <http://dbpedia.org/ontology/Actor> ;
          rdfs:comment """Relazione tra un personaggio e l'attore che lo interpreta.
Inversa di plays."""@it ;
          rdfs:label "playedBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#plays
:plays rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf owl:topObjectProperty ;
       rdfs:domain <http://dbpedia.org/ontology/Actor> ;
       rdfs:range <http://dbpedia.org/ontology/FictionalCharacter> ;
       rdfs:comment """Relazione tra un attore e il personaggio che interpreta.
Inversa di playedBy"""@it ;
       rdfs:label "plays"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#precedesEpisode
:precedesEpisode rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sequence:precedes ;
                 rdf:type owl:TransitiveProperty ;
                 rdfs:domain bbc-po:Episode ;
                 rdfs:range bbc-po:Episode ;
                 rdfs:comment "Relazione tra un episodio e un episodio successivo ad esso."@it ;
                 rdfs:label "precedesEpisode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#precedesSeason
:precedesSeason rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf sequence:precedes ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain bbc-po:Season ;
                rdfs:range bbc-po:Season ;
                rdfs:comment "Relazione tra una stagione e un'altra successiva ad essa."@it ;
                rdfs:label "precedesSeason"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#produces
:produces rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :productedBy ;
          rdfs:domain <http://dbpedia.org/ontology/Producer> ;
          rdfs:range bbc-po:Programme ;
          rdfs:comment """Relazione tra un produttore e uno show.
Inversa di productedBy"""@it ;
          rdfs:label "produces"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#productedBy
:productedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf bbc-po:producer ;
             rdfs:domain bbc-po:Programme ;
             rdfs:range <http://dbpedia.org/ontology/Producer> ;
             rdfs:comment "Relazione tra uno show e il suo produttore"@it ;
             rdfs:label "productedBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#receivedAward
:receivedAward rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :candidatedTo ;
               owl:inverseOf :wonBy ;
               rdfs:comment "Relazione tra il programma, l'attore o il regista e il premio che ha ricevuto"@it ;
               rdfs:label "receivedAward"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#reviewOf
:reviewOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Review ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( bbc-po:Episode
                                     :Film
                                   )
                     ] ;
          rdfs:comment "Relazione tra una recensione e l'episodio o film a cui si riferisce"@it ;
          rdfs:label "reviewOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#reviewWrittenBy
:reviewWrittenBy rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf :wroteReview ;
                 rdfs:domain :Review ;
                 rdfs:range :Reviewer ;
                 rdfs:comment "Relazione tra la recensione e la persona che l'ha scritta"@it ;
                 rdfs:label "reviewWrittenBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#seasonOf
:seasonOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf bag:itemOf ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain bbc-po:Season ;
          rdfs:range :TvSeries ;
          rdfs:comment "Relazione tra una stagione e la serie di cui fa parte"@it ;
          rdfs:label "seasonOf"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#showWrittenBy
:showWrittenBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :wroteShow ;
               rdfs:domain bbc-po:Programme ;
               rdfs:range <http://dbpedia.org/ontology/ScreenWriter> ;
               rdfs:comment "Relazione tra uno Show e lo Sceneggiatore che l'ha scritto."@it ;
               rdfs:label "showWrittenBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#wonBy
:wonBy rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :hasCandidate ;
       rdfs:comment """Indica la relazione tra un Premio e chi l'ha vinto.
Inversa di haRicevutoPremio"""@it ;
       rdfs:label "wonBy"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#wonFor
:wonFor rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :Award ;
        rdfs:range bbc-po:Programme ;
        rdfs:comment "Relazione tra un premio e il programma per cui è stato vinto il premio."@it ;
        rdfs:label "wonFor"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#wonForPlaying
:wonForPlaying rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :IndividualAward ;
               rdfs:range <http://dbpedia.org/ontology/FictionalCharacter> ;
               rdfs:comment "Indica la relazione tra un premio e il ruolo per cui è stato vinto"@it ;
               rdfs:label "wonForPlaying"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#writesFor
:writesFor rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :CriticReviewer ;
           rdfs:range :Magazine ;
           rdfs:comment "Relazione tra un critico e la rivista per cui scrive le recensioni"@it ;
           rdfs:label "writesFor"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#wroteReview
:wroteReview rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Reviewer ;
             rdfs:range :Review ;
             rdfs:comment "Relazione tra il recensore e la recensione che ha scritto"@it ;
             rdfs:label "wroteReview"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#wroteShow
:wroteShow rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain <http://dbpedia.org/ontology/ScreenWriter> ;
           rdfs:range bbc-po:Programme ;
           rdfs:comment "Relazione tra uno sceneggiatore e lo show che ha scritto"@it ;
           rdfs:label "wroteShow"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/ontology/po/synopsis
bbc-po:synopsis rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:domain bbc-po:Programme ;
                rdfs:range xsd:string ;
                rdfs:comment "Sinossi di un programma"@it ,
                             "The synopsis of a series, brand or episode."@en ;
                rdfs:label "synopsis"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#cerimonyDate
:cerimonyDate rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :FestivalEdition ;
              rdfs:range :xsd:date ;
              rdfs:comment "Data in cui si è tenuta l'edizione di una competizione"@it ;
              rdfs:label "cerimonyDate"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#distributionChannel
:distributionChannel rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :Film
                                                 :TvSeries
                                               )
                                 ] ;
                     rdfs:range xsd:string ;
                     rdfs:comment """Canale di distribuzione di un programma: cinema o tv. 
Nel caso della tv viene indicato la rete televisiva."""@it ;
                     rdfs:label "distributionChannel"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#duration
:duration rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( bbc-po:Episode
                                      :Film
                                    )
                      ] ;
          rdfs:range xsd:positiveInteger ;
          rdfs:comment "Durata in minuti dell'episodio o del film" ;
          rdfs:label "duration"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeCode
:episodeCode rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain bbc-po:Episode ;
             rdfs:range xsd:string ;
             rdfs:comment "Codice di produzione dell'episodio" ;
             rdfs:label "episodeCode"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#magazineName
:magazineName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Magazine ;
              rdfs:range xsd:string ;
              rdfs:comment "Nome di una rivista"@it ;
              rdfs:label "magazineName"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#officialName
:officialName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Award
                                          :AwardsFestival
                                          :FestivalEdition
                                        )
                          ] ;
              rdfs:range xsd:string ;
              rdfs:comment "Nome ufficiale di un festival, di un'edizione di un festival o di un premio"@it ;
              rdfs:label "officialName"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#originalLanguage
:originalLanguage rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Film
                                              :TvSeries
                                            )
                              ] ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Lingua originale del programma."@it ;
                  rdfs:label "originalLanguage(s)"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#releaseDate
:releaseDate rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( bbc-po:Episode
                                         :Film
                                       )
                         ] ;
             rdfs:range :xsd:date ;
             rdfs:comment "Data di uscita di un Episodio o di un Film"@it ;
             rdfs:label "releaseDate"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#reviewDescription
:reviewDescription rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdfs:domain :Review ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Descrizione di una recensione"@it ;
                   rdfs:label "reviewDescription"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#reviewRating
:reviewRating rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Review ;
              rdfs:range [ rdf:type rdfs:Datatype ;
                           owl:onDatatype xsd:integer ;
                           owl:withRestrictions ( [ xsd:minInclusive 0
                                                  ]
                                                  [ xsd:maxInclusive 10
                                                  ]
                                                )
                         ] ;
              rdfs:comment "Voto di una recensione da 0 a 10"@it ;
              rdfs:label "reviewRating"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#title
:title rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdfs:domain bbc-po:Programme ;
       rdfs:range xsd:string ;
       rdfs:comment "Titolo di un Programma"@it ;
       rdfs:label "title"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#totalEpisodes
:totalEpisodes rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( bbc-po:Season
                                           :TvSeries
                                         )
                           ] ;
               rdfs:range xsd:positiveInteger ;
               rdfs:comment "Numero di episodi da cui è composta una serie Tv o una stagione di una serie Tv"@it ;
               rdfs:label "totalEpisodes"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#totalSeasons
:totalSeasons rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :TvSeries ;
              rdfs:range xsd:positiveInteger ;
              rdfs:comment "Numero di stagioni da cui è composta una serie Tv"@it ;
              rdfs:label "totalSeasons"@en .


###  http://xmlns.com/foaf/0.1/age
foaf:age rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:positiveInteger ;
         rdfs:comment "Età di una persona"@it ;
         rdfs:label "age"@en .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:string ;
          rdfs:comment "Nome di una persona"@it ;
          rdfs:label "personName"@en .


###  http://xmlns.com/foaf/0.1/nick
foaf:nick rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Reviewer ;
          rdfs:range xsd:string ;
          rdfs:comment "Nickname di un utente"@it ;
          rdfs:label "nickname"@en .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Actor
<http://dbpedia.org/ontology/Actor> rdf:type owl:Class ;
                                    owl:equivalentClass [ owl:intersectionOf ( <http://dbpedia.org/ontology/Artist>
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :act_in ;
                                                                                 owl:someValuesFrom bbc-po:Programme
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ] ;
                                    rdfs:subClassOf <http://dbpedia.org/ontology/Artist> ;
                                    rdfs:comment "Persona che ha interpretato un ruolo in un programma cinematografico o televisivo"@it ;
                                    rdfs:label "Actor"@en .


###  http://dbpedia.org/ontology/Artist
<http://dbpedia.org/ontology/Artist> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://dbpedia.org/ontology/Person> ;
                                     rdfs:comment "Un artista è una persona che recita in una produzione teatrale, televisiva, cinematografica o radiofonica."@it ;
                                     rdfs:label "Artist"@en .


###  http://dbpedia.org/ontology/FictionalCharacter
<http://dbpedia.org/ontology/FictionalCharacter> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://dbpedia.org/ontology/Person> ;
                                                 rdfs:comment "Persona fittizia che compare in un qualche programma"@it ;
                                                 rdfs:label "Character"@en .


###  http://dbpedia.org/ontology/MovieDirector
<http://dbpedia.org/ontology/MovieDirector> rdf:type owl:Class ;
                                            owl:equivalentClass [ owl:intersectionOf ( :Director
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :directs ;
                                                                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                         owl:onClass :Film
                                                                                       ]
                                                                                     ) ;
                                                                  rdf:type owl:Class
                                                                ] ;
                                            rdfs:subClassOf :Director ;
                                            rdfs:comment "Regista che ha diretto almeno un film"@it ;
                                            rdfs:label "MovieDirector"@en .


###  http://dbpedia.org/ontology/Person
<http://dbpedia.org/ontology/Person> rdf:type owl:Class ;
                                     owl:equivalentClass bbc-po:Person ;
                                     rdfs:comment "Persona"@it ;
                                     rdfs:label "Person"@en .


###  http://dbpedia.org/ontology/Producer
<http://dbpedia.org/ontology/Producer> rdf:type owl:Class ;
                                       owl:equivalentClass [ owl:intersectionOf ( <http://dbpedia.org/ontology/Person>
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty :produces ;
                                                                                    owl:someValuesFrom bbc-po:Programme
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ] ;
                                       rdfs:comment "Produttore di uno show"@it ;
                                       rdfs:label "Producer"@en .


###  http://dbpedia.org/ontology/ScreenWriter
<http://dbpedia.org/ontology/ScreenWriter> rdf:type owl:Class ;
                                           owl:equivalentClass [ owl:intersectionOf ( <http://dbpedia.org/ontology/Writer>
                                                                                      [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty :wroteShow ;
                                                                                        owl:someValuesFrom bbc-po:Programme
                                                                                      ]
                                                                                    ) ;
                                                                 rdf:type owl:Class
                                                               ] ;
                                           rdfs:subClassOf <http://dbpedia.org/ontology/Writer> ;
                                           rdfs:comment "Sceneggiatore di uno show"@it ;
                                           rdfs:label "Screenwriter"@en .


###  http://dbpedia.org/ontology/TelevisionDirector
<http://dbpedia.org/ontology/TelevisionDirector> rdf:type owl:Class ;
                                                 owl:equivalentClass [ owl:intersectionOf ( :Director
                                                                                            [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty :directs ;
                                                                                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                              owl:onClass bbc-po:Episode
                                                                                            ]
                                                                                          ) ;
                                                                       rdf:type owl:Class
                                                                     ] ;
                                                 rdfs:subClassOf :Director ;
                                                 rdfs:comment "Regista che ha diretto almeno un episodio di una serie tv"@it ;
                                                 rdfs:label "TvSeriesDirector"@en .


###  http://dbpedia.org/ontology/Writer
<http://dbpedia.org/ontology/Writer> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://dbpedia.org/ontology/Person> ;
                                     rdfs:comment "Scrittore"@it ;
                                     rdfs:label "Writer"@en .


###  http://purl.org/ontology/po/Episode
bbc-po:Episode rdf:type owl:Class ;
               rdfs:subClassOf bbc-po:Programme ;
               rdfs:comment "Indica un episodio di una serie Tv"@it ;
               rdfs:label "Episode"@en .


###  http://purl.org/ontology/po/Genre
bbc-po:Genre rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:oneOf ( :Biografico
                                               :Comedy
                                               :Drama
                                               :Giallo
                                               :Sentimentale
                                               :Thriller
                                             )
                                 ] ;
             rdfs:comment "An anchor point for a programmes' genre taxonomy, e.g. 'Drama'/'Biographical'."@en ,
                          "Genere di un programma"@it ;
             rdfs:label "Genre"@en .


###  http://purl.org/ontology/po/Person
bbc-po:Person rdf:type owl:Class ;
              rdfs:comment "A person"@en ;
              rdfs:label "Person"@en .


###  http://purl.org/ontology/po/Programme
bbc-po:Programme rdf:type owl:Class ;
                 rdfs:comment "A programme, can either be a brand, a series or an episode"@en ,
                              "Produzione audiovisiva che viene trasmesso tramite un canale di distribuzione con finalità di informazione o intrattenimento."@it ;
                 rdfs:label "Programme"@en .


###  http://purl.org/ontology/po/Season
bbc-po:Season rdf:type owl:Class ;
              rdfs:subClassOf bbc-po:Programme ;
              rdfs:comment "Stagione di una serie TV" ;
              rdfs:label "Season"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Award
:Award rdf:type owl:Class ;
       rdfs:comment "Riconoscimento attribuito ad una persona o ad un programma"@it ;
       rdfs:label "Award"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#AwardsFestival
:AwardsFestival rdf:type owl:Class ;
                rdfs:comment "Festival che attribuisce dei premi riguardanti programmi cinematografici o televisivi."@it ;
                rdfs:label "AwardsFestival"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#BadReview
:BadReview rdf:type owl:Class ;
           rdfs:subClassOf :Review ;
           owl:disjointWith :GoodReview ;
           rdfs:comment "Recensione con voto < 6"@it ;
           rdfs:label "BadReview"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#CriticReview
:CriticReview rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :reviewWrittenBy ;
                                    owl:someValuesFrom :CriticReviewer
                                  ] ;
              rdfs:subClassOf :Review ;
              rdfs:comment "Recensione scritta da un qualche critico."@it ;
              rdfs:label "CriticReview"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#CriticReviewer
:CriticReviewer rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Reviewer
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :writesFor ;
                                                             owl:someValuesFrom :Magazine
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Reviewer ;
                rdfs:comment "Persona che scrive recensioni per una qualche rivista"@it ;
                rdfs:label "CriticReviewer"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Director
:Director rdf:type owl:Class ;
          rdfs:subClassOf <http://dbpedia.org/ontology/Person> ;
          rdfs:comment "Persona che ha diretto un film o un episodio di un serie tv"@it ;
          rdfs:label "Director"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#FIlmFestival
:FIlmFestival rdf:type owl:Class ;
              rdfs:subClassOf :AwardsFestival ;
              owl:disjointWith :TelevisionFestival ;
              rdfs:comment "Festival che atribuisce premi riguardanti programmi cinematografici"@it ;
              rdfs:label "FilmFestival"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#FestivalEdition
:FestivalEdition rdf:type owl:Class ;
                 rdfs:comment "Edizione di un festival che assegna premi"@it ;
                 rdfs:label "FestivalEdition"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Film
:Film rdf:type owl:Class ;
      rdfs:subClassOf bbc-po:Programme ;
      rdfs:comment "Programma che viene distribuito al cinema"@it ;
      rdfs:label "Film"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#GoodReview
:GoodReview rdf:type owl:Class ;
            rdfs:subClassOf :Review ;
            rdfs:comment "Recensione con voto >= 6"@it ;
            rdfs:label "GoodReview"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#IndividualAward
:IndividualAward rdf:type owl:Class ;
                 rdfs:subClassOf :Award ;
                 owl:disjointWith :ShowAward ;
                 rdfs:comment "Premio attribuito ad una persona"@it ;
                 rdfs:label "IndividualAward"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#LongTermTvSeries
:LongTermTvSeries rdf:type owl:Class ;
                  rdfs:subClassOf :TvSeries ;
                  rdfs:comment "Serie tv di lunga durata: almeno 5 stagioni"@it ;
                  rdfs:label "LongTermTvSeries"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Magazine
:Magazine rdf:type owl:Class ;
          rdfs:comment "Rivista su cui vengono pubblicate le recensioni"@it ;
          rdfs:label "Magazine"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Review
:Review rdf:type owl:Class ;
        rdfs:comment "Testo inteso ad illustrare e a giudicare criticamente un programma."@it ;
        rdfs:label "Review"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Reviewer
:Reviewer rdf:type owl:Class ;
          rdfs:subClassOf <http://dbpedia.org/ontology/Writer> ;
          rdfs:comment "Persona che scrive una recensione"@it ;
          rdfs:label "Reviewer"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#ShowAward
:ShowAward rdf:type owl:Class ;
           rdfs:subClassOf :Award ;
           rdfs:comment "Premio attribuito ad un programma televisivo o cinematografico"@it ;
           rdfs:label "ShowAward"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#TelevisionFestival
:TelevisionFestival rdf:type owl:Class ;
                    rdfs:subClassOf :AwardsFestival ;
                    rdfs:comment "Festival che atribuisce premi riguardanti programmi televisivi"@it ;
                    rdfs:label "TelevisionFestival"@en .


###  http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#TvSeries
:TvSeries rdf:type owl:Class ;
          rdfs:subClassOf bbc-po:Programme ;
          rdfs:comment "Programma che viene distribuito in televisione."@it ;
          rdfs:label "TvSeries"@en .


