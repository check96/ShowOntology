                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain so:IndividualAward ;
                 rdfs:range <http://dbpedia.org/ontology/FictionalCharacter> ;
                 rdfs:comment "Indica la relazione tra un premio e il ruolo per cui è stato vinto"@it ;
                 rdfs:label "wonForPlaying"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#writesFor
so:writesFor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain so:CriticReviewer ;
             rdfs:range so:Magazine ;
             rdfs:comment "Relazione tra un critico e la rivista per cui scrive le recensioni"@it ;
             rdfs:label "writesFor"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#wroteReview
so:wroteReview rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain so:Reviewer ;
               rdfs:range so:Review ;
               rdfs:comment "Relazione tra il recensore e la recensione che ha scritto"@it ;
               rdfs:label "wroteReview"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#wroteShow
so:wroteShow rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain <http://dbpedia.org/ontology/ScreenWriter> ;
             rdfs:range bbc-po:Programme ;
             rdfs:comment "Relazione tra uno sceneggiatore e lo show che ha scritto"@it ;
             rdfs:label "wroteShow"@en .


