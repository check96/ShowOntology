                rdfs:label "FilmFestival"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#FestivalEdition
so:FestivalEdition rdf:type owl:Class ;
                   rdfs:comment "Edizione di un festival che assegna premi"@it ;
                   rdfs:label "FestivalEdition"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#Film
so:Film rdf:type owl:Class ;
        rdfs:subClassOf bbc-po:Programme ;
        rdfs:comment "Programma che viene distribuito al cinema"@it ;
        rdfs:label "Film"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#GoodReview
so:GoodReview rdf:type owl:Class ;
              rdfs:subClassOf so:Review ;
              rdfs:comment "Recensione con voto >= 6"@it ;
              rdfs:label "GoodReview"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#IndividualAward
so:IndividualAward rdf:type owl:Class ;
                   rdfs:subClassOf so:Award ;
                   owl:disjointWith so:ShowAward ;
                   rdfs:comment "Premio attribuito ad una persona"@it ;
                   rdfs:label "IndividualAward"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#LongTermTvSeries
so:LongTermTvSeries rdf:type owl:Class ;
                    rdfs:subClassOf so:TvSeries ;
                    rdfs:comment "Serie tv di lunga durata: almeno 5 stagioni"@it ;
                    rdfs:label "LongTermTvSeries"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#Magazine
so:Magazine rdf:type owl:Class ;
            rdfs:comment "Rivista su cui vengono pubblicate le recensioni"@it ;
            rdfs:label "Magazine"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#Review
so:Review rdf:type owl:Class ;
          rdfs:comment "Testo inteso ad illustrare e a giudicare criticamente un programma."@it ;
          rdfs:label "Review"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#Reviewer
so:Reviewer rdf:type owl:Class ;
            rdfs:subClassOf <http://dbpedia.org/ontology/Writer> ;
            rdfs:comment "Persona che scrive una recensione"@it ;
            rdfs:label "Reviewer"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#ShowAward
so:ShowAward rdf:type owl:Class ;
             rdfs:subClassOf so:Award ;
             rdfs:comment "Premio attribuito ad un programma televisivo o cinematografico"@it ;
             rdfs:label "ShowAward"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#TelevisionFestival
so:TelevisionFestival rdf:type owl:Class ;
                      rdfs:subClassOf so:AwardsFestival ;
                      rdfs:comment "Festival che atribuisce premi riguardanti programmi televisivi"@it ;
                      rdfs:label "TelevisionFestival"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#TvSeries
so:TvSeries rdf:type owl:Class ;
            rdfs:subClassOf bbc-po:Programme ;
            rdfs:comment "Programma che viene distribuito in televisione."@it ;
            rdfs:label "TvSeries"@en .


