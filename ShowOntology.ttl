#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Actor
<http://dbpedia.org/ontology/Actor> rdf:type owl:Class ;
                                    owl:equivalentClass [ owl:intersectionOf ( <http://dbpedia.org/ontology/Artist>
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty so:act_in ;
                                                                                 owl:someValuesFrom bbc-po:Programme
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ] ;
                                    rdfs:subClassOf <http://dbpedia.org/ontology/Artist> ;
                                    rdfs:comment "Persona che ha interpretato un ruolo in un programma cinematografico o televisivo"@it ;
                                    rdfs:label "Actor"@en .


###  http://dbpedia.org/ontology/Artist
<http://dbpedia.org/ontology/Artist> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://dbpedia.org/ontology/Person> ;
                                     rdfs:comment "Un artista è una persona che recita in una produzione teatrale, televisiva, cinematografica o radiofonica."@it ;
                                     rdfs:label "Artist"@en .


###  http://dbpedia.org/ontology/FictionalCharacter
<http://dbpedia.org/ontology/FictionalCharacter> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://dbpedia.org/ontology/Person> ;
                                                 rdfs:comment "Persona fittizia che compare in un qualche programma"@it ;
                                                 rdfs:label "Character"@en .


###  http://dbpedia.org/ontology/MovieDirector
<http://dbpedia.org/ontology/MovieDirector> rdf:type owl:Class ;
                                            owl:equivalentClass [ owl:intersectionOf ( so:Director
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty so:directs ;
                                                                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                         owl:onClass so:Film
                                                                                       ]
                                                                                     ) ;
                                                                  rdf:type owl:Class
                                                                ] ;
                                            rdfs:subClassOf so:Director ;
                                            rdfs:comment "Regista che ha diretto almeno un film"@it ;
                                            rdfs:label "MovieDirector"@en .


###  http://dbpedia.org/ontology/Person
<http://dbpedia.org/ontology/Person> rdf:type owl:Class ;
                                     owl:equivalentClass bbc-po:Person ;
                                     rdfs:comment "Persona"@it ;
                                     rdfs:label "Person"@en .


###  http://dbpedia.org/ontology/Producer
<http://dbpedia.org/ontology/Producer> rdf:type owl:Class ;
                                       owl:equivalentClass [ owl:intersectionOf ( <http://dbpedia.org/ontology/Person>
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty so:produces ;
                                                                                    owl:someValuesFrom bbc-po:Programme
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ] ;
                                       rdfs:comment "Produttore di uno show"@it ;
                                       rdfs:label "Producer"@en .


###  http://dbpedia.org/ontology/ScreenWriter
<http://dbpedia.org/ontology/ScreenWriter> rdf:type owl:Class ;
                                           owl:equivalentClass [ owl:intersectionOf ( <http://dbpedia.org/ontology/Writer>
                                                                                      [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty so:wroteShow ;
                                                                                        owl:someValuesFrom bbc-po:Programme
                                                                                      ]
                                                                                    ) ;
                                                                 rdf:type owl:Class
                                                               ] ;
                                           rdfs:subClassOf <http://dbpedia.org/ontology/Writer> ;
                                           rdfs:comment "Sceneggiatore di uno show"@it ;
                                           rdfs:label "Screenwriter"@en .


###  http://dbpedia.org/ontology/TelevisionDirector
<http://dbpedia.org/ontology/TelevisionDirector> rdf:type owl:Class ;
                                                 owl:equivalentClass [ owl:intersectionOf ( so:Director
                                                                                            [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty so:directs ;
                                                                                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                              owl:onClass bbc-po:Episode
                                                                                            ]
                                                                                          ) ;
                                                                       rdf:type owl:Class
                                                                     ] ;
                                                 rdfs:subClassOf so:Director ;
                                                 rdfs:comment "Regista che ha diretto almeno un episodio di una serie tv"@it ;
                                                 rdfs:label "TvSeriesDirector"@en .


###  http://dbpedia.org/ontology/Writer
<http://dbpedia.org/ontology/Writer> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://dbpedia.org/ontology/Person> ;
                                     rdfs:comment "Scrittore"@it ;
                                     rdfs:label "Writer"@en .


###  http://purl.org/ontology/po/Episode
bbc-po:Episode rdf:type owl:Class ;
               rdfs:subClassOf bbc-po:Programme ;
               rdfs:comment "Indica un episodio di una serie Tv"@it ;
               rdfs:label "Episode"@en .


###  http://purl.org/ontology/po/Genre
bbc-po:Genre rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:oneOf ( so:Biografico
                                               so:Comedy
                                               so:Drama
                                               so:Giallo
                                               so:Sentimentale
                                               so:Thriller
                                             )
                                 ] ;
             rdfs:comment "An anchor point for a programmes' genre taxonomy, e.g. 'Drama'/'Biographical'."@en ,
                          "Genere di un programma"@it ;
             rdfs:label "Genre"@en .


###  http://purl.org/ontology/po/Person
bbc-po:Person rdf:type owl:Class ;
              rdfs:comment "A person"@en ;
              rdfs:label "Person"@en .


###  http://purl.org/ontology/po/Programme
bbc-po:Programme rdf:type owl:Class ;
                 rdfs:comment "A programme, can either be a brand, a series or an episode"@en ,
                              "Produzione audiovisiva che viene trasmesso tramite un canale di distribuzione con finalità di informazione o intrattenimento."@it ;
                 rdfs:label "Programme"@en .


###  http://purl.org/ontology/po/Season
bbc-po:Season rdf:type owl:Class ;
              rdfs:subClassOf bbc-po:Programme ;
              rdfs:comment "Stagione di una serie TV" ;
              rdfs:label "Season"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#Award
so:Award rdf:type owl:Class ;
         rdfs:comment "Riconoscimento attribuito ad una persona o ad un programma"@it ;
         rdfs:label "Award"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#AwardsFestival
so:AwardsFestival rdf:type owl:Class ;
                  rdfs:comment "Festival che attribuisce dei premi riguardanti programmi cinematografici o televisivi."@it ;
                  rdfs:label "AwardsFestival"@en .


###  http://www.semanticweb.org/gaetano/ontologies/ShowOntology#BadReview
so:BadReview rdf:type owl:Class ;
