    <!-- http://dbpedia.org/ontology/Actor -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Actor">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Artist"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#act_in"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/ontology/po/Programme"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://dbpedia.org/ontology/Artist"/>
        <rdfs:comment xml:lang="it">Persona che ha interpretato un ruolo in un programma cinematografico o televisivo</rdfs:comment>
        <rdfs:label xml:lang="en">Actor</rdfs:label>
    </owl:Class>
    


