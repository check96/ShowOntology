<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#"
     xml:base="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:so="http://www.semanticweb.org/gaetano/ontologies/ShowOntology#"
     xmlns:bag="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:list="http://www.ontologydesignpatterns.org/cp/owl/list.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:spec="http://xmlns.com/foaf/spec/#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:vann="https://vocab.org/vann/#"
     xmlns:foaf1="http://xmlns.com/foaf/"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:bbc-po="http://purl.org/ontology/po/"
     xmlns:sequence="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#"
     xmlns:collectionentity="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl"/>
        <dc:creator>Gaetano Donadio</dc:creator>
        <dc:date>07-09-2020</dc:date>
        <dc:description xml:lang="it">Ontologia per la descrizione e rappresentazione di entità relative al mondo cinematografico e televisivo. Sono rappresentati programmi come Film e Serie Tv e la relativa valutazione: recensioni e premi attribuiti.</dc:description>
        <dc:title>Show Ontology</dc:title>
        <rdfs:label>Show Ontology</rdfs:label>
        <vann:preferredNamespacePrefix>so</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#</vann:preferredNamespaceUri>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- https://vocab.org/vann/#preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="https://vocab.org/vann/#preferredNamespacePrefix"/>
    


    <!-- https://vocab.org/vann/#preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="https://vocab.org/vann/#preferredNamespaceUri"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#xsd:date -->

    <rdfs:Datatype rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#xsd:date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/po/director -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/po/director">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Person"/>
        <rdfs:comment xml:lang="en">Relates a programme to its supervisor. Generally refers to the person responsible for all audience-visible components of a program, film, or show, whereas the producer is responsible for the financial and other behind-the-scenes aspects. A director&apos;s duties might also include casting, script editing, shot selection, shot composition, and editing</rdfs:comment>
        <rdfs:label xml:lang="en">director</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/po/genre -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/po/genre">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Genre"/>
        <rdfs:comment xml:lang="en">Relates a programme to a particular genre.</rdfs:comment>
        <rdfs:label xml:lang="en">genre</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/po/producer -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/po/producer">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Person"/>
        <rdfs:comment xml:lang="en">Relates a programme to its producer - the manager of an event, show, or other work, usually the individual in charge of finance, personnel, and other non-artistic aspects in the development of commercials, plays, movies, and other works</rdfs:comment>
        <rdfs:label xml:lang="en">producer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#act_in -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#act_in">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Actor"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#plays"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#characterOf"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Relazione tra un attore e il programma (FIlm o Serie Tv) in cui recita.</rdfs:comment>
        <rdfs:label xml:lang="en">act_in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#appears_in -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#appears_in">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/FictionalCharacter"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:comment>Relazione tra un personaggio e l&apos;episodio in cui appare</rdfs:comment>
        <rdfs:label xml:lang="en">appears_in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#assigned_in -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#assigned_in">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Award"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#FestivalEdition"/>
        <rdfs:comment xml:lang="it">Indica la relazione tra un premio e l&apos;edizione del festival a cui è stato assegnato</rdfs:comment>
        <rdfs:label xml:lang="en">assigned_in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#candidatedTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#candidatedTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasCandidate"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/po/Person"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/po/Programme"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Award"/>
        <rdfs:comment xml:lang="en">Relazione tra un Programma, un Attore o un Regista e il premio per cui è candidato alla vittoria.
Relazione inversa di hasCandidated</rdfs:comment>
        <rdfs:label xml:lang="en">candidatedTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#characterOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#characterOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasCharacter"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/FictionalCharacter"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Film"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#TvSeries"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Relazione tra un personaggio e il film o la serie tv in cui compare</rdfs:comment>
        <rdfs:label xml:lang="en">characterOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#directedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#directedBy">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/po/director"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#directs"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/po/Episode"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Film"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Director"/>
        <rdfs:comment xml:lang="it">Relazione tra un episodio o un film e il regista che l&apos;ha diretto.
Inversa di directs</rdfs:comment>
        <rdfs:label xml:lang="en">directedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#directs -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#directs">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Director"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/po/Episode"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Film"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Relazione tra il regista e l&apos;episodio o film che ha diretto.
Inversa di directedBy</rdfs:comment>
        <rdfs:label xml:lang="en">directs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#editionOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#editionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasEdition"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#FestivalEdition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#AwardsFestival"/>
        <rdfs:comment xml:lang="it">Indica la relazione tra un&apos;edizione di una competizione e la competizione stessa</rdfs:comment>
        <rdfs:label xml:lang="en">editionOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasEpisode"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Season"/>
        <rdfs:comment xml:lang="it">Relazione tra un episodio e la stagione di cui fa parte</rdfs:comment>
        <rdfs:label xml:lang="en">episodeOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeOfSeries -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeOfSeries">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#TvSeries"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeOf"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#seasonOf"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Relazione tra un episodio e la serie tv di cui fa parte</rdfs:comment>
        <rdfs:label xml:lang="en">episodeOfSeries</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#firstEpisodeOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#firstEpisodeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl#firstItemOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#episodeOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasFirstEpisode"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Season"/>
        <rdfs:comment xml:lang="it">Relazione inversa di hasFirstEpisode</rdfs:comment>
        <rdfs:label xml:lang="en">firstEpisodeOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#firstSeasonOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#firstSeasonOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl#firstItemOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#seasonOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasFirstSeason"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Season"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#TvSeries"/>
        <rdfs:comment xml:lang="it">Relazione inversa di hasFirstSeason</rdfs:comment>
        <rdfs:label xml:lang="en">firstSeasonOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#followsEpisode -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#followsEpisode">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#follows"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#precedesEpisode"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:comment xml:lang="it">Relazione tra un episodio e un episodio precedente ad esso.</rdfs:comment>
        <rdfs:label xml:lang="en">followsEpisode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#followsSeason -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#followsSeason">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#follows"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#precedesSeason"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Season"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Season"/>
        <rdfs:comment xml:lang="it">Relazione tra una stagione e un&apos;altra precedente ad essa..</rdfs:comment>
        <rdfs:label xml:lang="en">followsSeason</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasCandidate -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasCandidate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Award"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/po/Person"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/po/Programme"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Relazione tra un premio e un candidato a vincerlo.
Inversa di candidatedTo</rdfs:comment>
        <rdfs:label xml:lang="en">hasCandidate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasCharacter -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasCharacter">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Film"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#TvSeries"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/FictionalCharacter"/>
        <rdfs:comment xml:lang="it">Relazione tra un film o una serie Tv e i personaggi che appaiono in esso/a.
Inversa di characterOf</rdfs:comment>
        <rdfs:label xml:lang="en">hasCharacter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasEdition -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#hasEdition">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#AwardsFestival"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#FestivalEdition"/>
        <rdfs:comment xml:lang="it">Relazione tra una competizione a premi e un&apos;edizione di essa.
Inversa di editionOf</rdfs:comment>
        <rdfs:label xml:lang="en">hasEdition</rdfs:label>
    </owl:ObjectProperty>
    


