    <!-- http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Review10 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Review10">
        <reviewOf rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Joker"/>
        <reviewWrittenBy rdf:resource="http://www.semanticweb.org/gaetano/ontologies/2020/8/ShowOntology#Luis_Bond"/>
        <reviewDescription>Joker is dark, dense, violent, and monstrous. A descent into Hades that is unlike anything we&apos;ve seen on screen that&apos;s inspired by a comic</reviewDescription>
        <reviewRating rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</reviewRating>
        <rdfs:comment xml:lang="it">Recensione</rdfs:comment>
        <rdfs:label xml:lang="en">Review10</rdfs:label>
